{
	"Configuration": {
		"LogicMonitorCredentials": {
			"Account": "<accountname>",
			"AccessId": "<access id>",
			"AccessKey": "<access key>"
		},
		"Mode": "Delete",
		"Variables": [
			{
				"Name": "customer.code",
				"Value": "ACM"
			},
			{
				"Name": "customer.name",
				"Value": "ACME Test Ltd"
			},
			{
				"Name": "customer.description",
				"Value": "ACME Test Ltd is a very fine company indeed"
			},
			{
				"Name": "customer.autotask.id",
				"Value": "1234"
			}
		],
		"Properties": [
			{
				"Name": "customer.id",
				"Value": "{customer.id}"
			},
			{
				"Name": "autotask.id",
				"Value": "{customer.autotask.id}"
			}
		],
		"Collectors": {
			"Enabled": true,
			"Name": "{customer.code} - {customer.name}",
			"Description": "{customer.description}"
		},
		"Dashboards": {
			"Enabled": true,
			"Name": "{customer.code} - {customer.name}",
			"Groups": [
				{
					"Name": "Network",
					"Items": [
						{
							"Name": "{customer.name} Network",
							"CloneFromId": 74
						}
					]
				},
				{
					"Name": "Servers"
				},
				{
					"Name": "Storage"
				}
			]
		},
		"Devices": {
			"Enabled": true,
			"Name": "{customer.code} - {customer.name}",
			"Groups": [
				{
					"Name": "Collector",
					"AppliesTo": "type == \"collector\" && customer.id == \"{customer.id}\"",
					"Groups": [
						{
							"Name": "Collector subgroup"
						}
					]
				},
				{
					"Name": "Network",
					"AppliesTo": "type == \"network\" && customer.id == \"{customer.id}\""
				},
				{
					"Name": "Servers",
					"AppliesTo": "type == \"server\" && customer.id == \"{customer.id}\""
				},
				{
					"Name": "Storage",
					"AppliesTo": "type == \"storage\" && customer.id == \"{customer.id}\""
				}
			]
		},
		"Netscans": {
			"Enabled": true,
			"Name": "{customer.code} - {customer.name}",
			"ImportItemsFrom": "{customer.name} Netscans.xlsx"
		},
		"Reports": {
			"Enabled": true,
			"Name": "{customer.code} - {customer.name}"
		},
		"Websites": {
			"Enabled": true,
			"Name": "{customer.code} - {customer.name}",
			"Groups": [
				{
					"Name": "Intranet"
				},
				{
					"Name": "Extranet"
				}
			]
		},
		"RoleConfigurations": [
			{
				"Name": "{customer.name} - View",
				"Description": "View only permissions for {customer.name}.",
				"IsEnabled": true,
				"AccessLevel": "Read"
			},
			{
				"Name": "{customer.name} - Manage",
				"Description": "Manage permissions for {customer.name}.",
				"IsEnabled": true,
				"AccessLevel": "Write"
			}
		]
	},
	"Logging": {
		"LogLevel": {
			"Default": "Information"
		}
	}
}
